{
  "name": "umodelerx-backend",
  "version": "1.0.0",
  "description": "UModelerX AI 기반 Nest.js 프로젝트",
  "scripts": {
    "cron:dev": "node cron.js",
    "start": "nest start",
    "build": "nest build",
    "dev": "nest start --watch",
    "nest:gen": "nest g res",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate -d ./data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ./data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d ./data-source.ts"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/storage": "^7.17.3",
    "@google-cloud/tasks": "^6.2.1",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/typeorm": "^11.0.0",
    "@types/jest": "^30.0.0",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.0",
    "google-auth-library": "^10.5.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "mysql2": "^3.14.0",
    "nestjs-i18n": "^10.5.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sanitize-html": "^2.17.0",
    "typeorm": "^0.3.0",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
